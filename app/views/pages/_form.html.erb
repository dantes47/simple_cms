<%= error_messages(form.object) %>

<table summary="Page from Fields">
  <tr>
    <th>Subject ID</th>
    <td><%= form.text_field(:subject_id) %></td>
  </tr>
  <tr>
    <th>Name</th>
    <td><%= form.text_field(:name) %></td>
  </tr>
  <tr>
    <th>Permalink</th>
    <td><%= form.text_field(:permalink)%></td>
  </tr>
  <tr>
    <th>Position</th>
    <td><%= form.text_field(:position) %></td>
  </tr>
  <tr>
    <th>Visible</th>
    <td><%= form.text_field(:visible) %></td>
  </tr>
  <tr>
    <th>Content</th>
    <td><%= form.text_area(:content, rows: 10, cols: 60) %></td>
  </tr>
</table>
